    <Setting Name="IncomingFaxDirectory" Description="The directory where the incoming faxes are stored." Type="System.String" Scope="Application">
      <Value Profile="(Default)">C:\Documents and Settings\All Users\Application Data\ClearCanvas_Inc\IncomingFax</Value>
    </Setting>
    <Setting Name="MetaDataFileExtension" Description="Customizable fax metadata file extension." Type="System.String" Scope="Application">
      <Value Profile="(Default)">fmd</Value>
    </Setting>
    <Setting Name="ImageDataFileExtension" Description="Image data file extension (e.g. pdf, tif, jpg)" Type="System.String" Scope="Application">
      <Value Profile="(Default)">pdf</Value>
    </Setting>
    <Setting Name="ErrorFaxDirectory" Description="The directory where the faxes with errors are stored." Type="System.String" Scope="Application">
      <Value Profile="(Default)">C:\Documents and Settings\All Users\Application Data\ClearCanvas_Inc\ErrorFax</Value>
    </Setting>
    <Setting Name="MetaDataDelimiters" Description="The delimiters used to separate field name and value in the meta data file." Type="System.String" Scope="Application">
      <Value Profile="(Default)">=</Value>
    </Setting>
  </Settings>
</SettingsFile>