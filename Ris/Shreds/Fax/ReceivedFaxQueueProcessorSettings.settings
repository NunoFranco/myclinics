<?xml version='1.0' encoding='utf-8'?>
<SettingsFile xmlns="http://schemas.microsoft.com/VisualStudio/2004/01/settings" CurrentProfile="(Default)" GeneratedClassNamespace="ClearCanvas.Ris.Shreds.Fax" GeneratedClassName="ReceivedFaxQueueProcessorSettings">
  <Profiles />
  <Settings>
    <Setting Name="BatchSize" Description="Batch size for processor to retrieve." Type="System.Int32" Scope="Application">
      <Value Profile="(Default)">20</Value>
    </Setting>
    <Setting Name="SleepTime" Description="Processor thread sleep time in seconds." Type="System.Int32" Scope="Application">
      <Value Profile="(Default)">10</Value>
    </Setting>
    <Setting Name="IgnoreSpam" Description="Dictates processor action regarding spam fax items." Type="System.Boolean" Scope="Application">
