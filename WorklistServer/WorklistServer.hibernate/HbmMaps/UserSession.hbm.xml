<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="WorklistServer.hibernate.BusinessObjects" assembly="WorklistServer.hibernate">
	<class name="WorklistServer.hibernate.BusinessObjects.UserSession, WorklistServer.hibernate" table="dbo.UserSession_" lazy="true">
		<id name="Id" column="OID_">
		 	<generator class="assigned" />
		</id>
		<property name="HostName" column="HostName_" />
		<property name="Application" column="Application_" />
		<property name="SessionId" column="SessionId_" />
		<property name="CreationTime" column="CreationTime_" />
		<property name="ExpiryTime" column="ExpiryTime_" />
		<many-to-one name="User" column="UserOID_" class="User" />
	</class>
</hibernate-mapping>