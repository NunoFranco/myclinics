<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="WorklistServer.hibernate.BusinessObjects" assembly="WorklistServer.hibernate">
	<class name="WorklistServer.hibernate.BusinessObjects.ProcedureType, WorklistServer.hibernate" table="dbo.ProcedureType_" lazy="true">
		<id name="Id" column="OID_">
		 	<generator class="assigned" />
		</id>
		<property name="Version" column="Version_" />
		<property name="Id" column="Id_" />
		<property name="Name" column="Name_" />
		<property name="PlanXml" column="PlanXml_" />
		<property name="Deactivated" column="Deactivated_" />
		<many-to-one name="ProcedureTypeMember" column="BaseTypeOID_" class="ProcedureType" />
		<bag name="Procedures" lazy="true" cascade="all-delete-orphan" inverse="true" >
			<key column="ProcedureTypeOID_"></key>
			<one-to-many class="Procedure"></one-to-many>
		</bag>
	</class>
</hibernate-mapping>