<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="WorklistServer.hibernate.BusinessObjects" assembly="WorklistServer.hibernate">
	<class name="WorklistServer.hibernate.BusinessObjects.InformationAuthorityEnum, WorklistServer.hibernate" table="dbo.InformationAuthorityEnum_" lazy="true">
		<id name="Id" column="Code_">
		 	<generator class="assigned" />
		</id>
		<property name="Value" column="Value_" />
		<property name="Description" column="Description_" />
		<property name="DisplayOrder" column="DisplayOrder_" />
		<property name="Deactivated" column="Deactivated_" />
		<bag name="Facilities" lazy="true" cascade="all-delete-orphan" inverse="true" >
			<key column="InformationAuthority_"></key>
			<one-to-many class="Facility"></one-to-many>
		</bag>
		<bag name="PatientProfileExpiredMrns" lazy="true" cascade="all-delete-orphan" inverse="true" >
			<key column="MrnAssigningAuthority_"></key>
			<one-to-many class="PatientProfileExpiredMrn"></one-to-many>
		</bag>
		<bag name="PatientProfiles" lazy="true" cascade="all-delete-orphan" inverse="true" >
			<key column="MrnAssigningAuthority_"></key>
			<one-to-many class="PatientProfile"></one-to-many>
		</bag>
		<bag name="Visits" lazy="true" cascade="all-delete-orphan" inverse="true" >
			<key column="VisitNumberAssigningAuthority_"></key>
			<one-to-many class="Visit"></one-to-many>
		</bag>
	</class>
</hibernate-mapping>