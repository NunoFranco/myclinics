<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="WorklistServer.hibernate.BusinessObjects" assembly="WorklistServer.hibernate">
	<class name="WorklistServer.hibernate.BusinessObjects.CannedText, WorklistServer.hibernate" table="dbo.CannedText_" lazy="true">
		<id name="Id" column="OID_">
		 	<generator class="assigned" />
		</id>
		<property name="Version" column="Version_" />
		<property name="Name" column="Name_" />
		<property name="Category" column="Category_" />
		<property name="Text" column="Text_" />
		<many-to-one name="StaffGroup" column="StaffGroupOID_" class="StaffGroup" />
		<many-to-one name="Staff" column="StaffOID_" class="Staff" />
	</class>
</hibernate-mapping>