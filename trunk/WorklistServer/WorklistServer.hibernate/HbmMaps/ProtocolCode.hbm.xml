<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="WorklistServer.hibernate.BusinessObjects" assembly="WorklistServer.hibernate">
	<class name="WorklistServer.hibernate.BusinessObjects.ProtocolCode, WorklistServer.hibernate" table="dbo.ProtocolCode_" lazy="true">
		<id name="Id" column="OID_">
		 	<generator class="assigned" />
		</id>
		<property name="Version" column="Version_" />
		<property name="Name" column="Name_" />
		<property name="Description" column="Description_" />
		<property name="Deactivated" column="Deactivated_" />
		<bag name="ProtocolGroups" table="dbo.ProtocolGroupProtocolCode_" lazy="true" cascade="all" inverse="false" >
			<key column="ProtocolCodeOID_"></key>
			<many-to-many column="ProtocolGroupOID_" class="ProtocolGroup" />
    	</bag>
	</class>
</hibernate-mapping>