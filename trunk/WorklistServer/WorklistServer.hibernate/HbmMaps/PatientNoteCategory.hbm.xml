<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="WorklistServer.hibernate.BusinessObjects" assembly="WorklistServer.hibernate">
	<class name="WorklistServer.hibernate.BusinessObjects.PatientNoteCategory, WorklistServer.hibernate" table="dbo.PatientNoteCategory_" lazy="true">
		<id name="Id" column="OID_">
		 	<generator class="assigned" />
		</id>
		<property name="Version" column="Version_" />
		<property name="Name" column="Name_" />
		<property name="Description" column="Description_" />
		<property name="Deactivated" column="Deactivated_" />
		<many-to-one name="NoteSeverityEnum" column="Severity_" class="NoteSeverityEnum" />
		<bag name="PatientNotes" lazy="true" cascade="all-delete-orphan" inverse="true" >
			<key column="CategoryOID_"></key>
			<one-to-many class="PatientNote"></one-to-many>
		</bag>
	</class>
</hibernate-mapping>