<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="WorklistServer.hibernate.BusinessObjects" assembly="WorklistServer.hibernate">
	<class name="WorklistServer.hibernate.BusinessObjects.ReportPart, WorklistServer.hibernate" table="dbo.ReportPart_" lazy="true">
		<id name="Id" column="OID_">
		 	<generator class="assigned" />
		</id>
		<property name="Version" column="Version_" />
		<property name="Index" column="Index_" />
		<property name="CreationTime" column="CreationTime_" />
		<property name="PreliminaryTime" column="PreliminaryTime_" />
		<property name="CompletedTime" column="CompletedTime_" />
		<property name="CancelledTime" column="CancelledTime_" />
		<many-to-one name="Staff1" column="InterpreterOID_" class="Staff" />
		<many-to-one name="Report" column="ReportOID_" class="Report" />
		<many-to-one name="ReportPartStatusEnum" column="Status_" class="ReportPartStatusEnum" />
		<many-to-one name="Staff2" column="SupervisorOID_" class="Staff" />
		<many-to-one name="Staff3" column="TranscriberOID_" class="Staff" />
		<many-to-one name="TranscriptionRejectReasonEnum" column="TranscriptionRejectReason_" class="TranscriptionRejectReasonEnum" />
		<many-to-one name="Staff4" column="TranscriptionSupervisorOID_" class="Staff" />
		<many-to-one name="Staff5" column="VerifierOID_" class="Staff" />
		<bag name="ProcedureSteps" lazy="true" cascade="all" inverse="true" >
			<key column="ReportPartOID_"></key>
			<one-to-many class="ProcedureStep"></one-to-many>
		</bag>
	</class>
</hibernate-mapping>