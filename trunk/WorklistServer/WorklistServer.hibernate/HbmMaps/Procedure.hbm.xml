<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="WorklistServer.hibernate.BusinessObjects" assembly="WorklistServer.hibernate">
	<class name="WorklistServer.hibernate.BusinessObjects.Procedure, WorklistServer.hibernate" table="dbo.Procedure_" lazy="true">
		<id name="Id" column="OID_">
		 	<generator class="assigned" />
		</id>
		<property name="Version" column="Version_" />
		<property name="Index" column="Index_" />
		<property name="ScheduledStartTime" column="ScheduledStartTime_" />
		<property name="StartTime" column="StartTime_" />
		<property name="EndTime" column="EndTime_" />
		<property name="Portable" column="Portable_" />
		<property name="DowntimeRecoveryMode" column="DowntimeRecoveryMode_" />
		<many-to-one name="ImageAvailabilityEnum" column="ImageAvailability_" class="ImageAvailabilityEnum" />
		<many-to-one name="LateralityEnum" column="Laterality_" class="LateralityEnum" />
		<many-to-one name="Order" column="OrderOID_" class="Order" />
		<many-to-one name="Facility" column="PerformingFacilityOID_" class="Facility" />
		<many-to-one name="ProcedureCheckIn" column="ProcedureCheckInOID_" class="ProcedureCheckIn" />
		<many-to-one name="ProcedureType" column="ProcedureTypeOID_" class="ProcedureType" />
		<many-to-one name="ProcedureStatusEnum" column="Status_" class="ProcedureStatusEnum" />
		<bag name="ProcedureSteps" lazy="true" cascade="all-delete-orphan" inverse="true" >
			<key column="ProcedureOID_"></key>
			<one-to-many class="ProcedureStep"></one-to-many>
		</bag>
	</class>
</hibernate-mapping>