<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ClearCanvas.Healthcare" namespace="ClearCanvas.Healthcare">
  <class name="Currency" proxy="Currency" table="BillingCurrency_">
    <id name="OID" column="CurrencyID_" type="Guid">
      <generator class="guid.comb"/>
    </id>
    <property name="CurrencyCode" column="CurrencyCode_" type="String" length="10" not-null="true"/>
    <property name="CurrencyName" column="CurrencyName_" type="String" length="50" not-null="true"/>
    <property name="RateToPrimaryExRate" column="RateToPrimaryExRate_" type="Decimal" not-null="true"/>
    <property name="DisplayLocale" column="DisplayLocale_" type="String" length="50" not-null="false"/>
    <property name="CustomDisplayFormat" column="CustomDisplayFormat_" type="String" length="50" not-null="false"/>
    <property name="IsPrimaryExRateCurrency" column="IsPrimaryExRateCurrency_" type="Boolean" not-null="true"/>
    <property name="IsPrimaryCurrency" column="IsPrimaryCurrency_" type="Boolean" not-null="true"/>
    <property name="CreatedUser" column="CreatedUser_" type="String" not-null="false"/>
    <property name="Deactivated" column="Deactivated_" type="Boolean" not-null="false"/>
    <property name="CreatedOn" column="CreatedOn_" type="DateTime" not-null="true"/>
    <property name="LastUpdated" column="lastUpdated_" type="DateTime" not-null="true"/>
    <many-to-one name="Clinic" class="Facility" column="ClinicOID_" not-null="false" fetch="join"/>
    
  </class>
</hibernate-mapping>
