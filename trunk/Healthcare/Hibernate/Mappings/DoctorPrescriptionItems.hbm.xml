<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ClearCanvas.Healthcare" namespace="ClearCanvas.Healthcare">
  <class name="DoctorPrescriptionItems" proxy="DoctorPrescriptionItems" table="DoctorPrescriptionItems_">
    <cache usage="nonstrict-read-write"/>
    <id name="OID" column="OID_" type="Guid">
      <generator class="guid.comb"/>
    </id>
    <version name="Version" column="Version_"/>
    
    <property name="Description" column="Description_" type="String"/>
    <property name="TimesPerday" column="TimesPerday_" type="String"/>
    <property name="AmountperTime" column="AmountperTime_" type="String"/>
    <many-to-one name="Medicines" class="ProcedureType" column="MedicineOID_"/>
    <many-to-one name="Prescription" class="DoctorPrescription" column="DoctorPrescriptionOID_"/>
    <property name="Deactivated" column="Deactivated_" type="Boolean" not-null="true"/>
  </class>
</hibernate-mapping>
